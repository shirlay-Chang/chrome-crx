{
	"manifest_version": 2,
	"name": "weather",
	"version": "1.0",
	"content_scripts": [{
		"matches": ["http://*/*","https://*/*"],
        "js": [
            "jquery-3.1.1.min.js"
        ]
    }],
	"description": "The first extension that I made.",
	"browser_action": {  
    "default_icon": "icon.png" ,
    "default_title": "weather",
    "default_popup": "popup.html"
    } ,
	"permissions": [
	"http://api.flickr.com/"
    ]
}